<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.course.model">

<!--    获取登录接口case-->
    <select id="loginTrue" parameterType="Integer" resultType="com.sourse.model.LoginCase">
        select * from logincase where id = #{id}
    </select>

<!--    添加用户接口case-->
    <select id="addUserCase" parameterType="Integer" resultType="com.sourse.model.AddUserCase">
        select * from addusercase where id = #{id}
    </select>

<!--    获取用户信息case-->
    <select id="getUserInfo" parameterType="Integer" resultType="com.sourse.model.GetUserInfoCase">
        select * from getuserinfocase where id = #{id}
    </select>

<!--    获取用户列表case-->
    <select id="getUserListCase" parameterType="Integer" resultType="com.sourse.model.GetUserListCase">
        select * from getuserlistcase where id = #{id}
    </select>

<!--    更新删除用户case-->
    <select id="upDateUserInfoCase" parameterType="Integer" resultType="com.sourse.model.UpdateUserInfoCase">
        select * from updateuserinfocase where id = #{id}
    </select>
</mapper>
